{
  "name": "premiser-api",
  "version": "0.1.0",
  "description": "API for premiser.co.  Proprietary and confidential.  Copyright Carl Gieringer",
  "main": "index.js",
  "repository": "git@bitbucket.org:premiser/premiser.git",
  "author": "Carl Gieringer <carl@premiser.co>",
  "license": "UNLICENSED",
  "scripts": {
    "db:local:start": "pg_ctl -D /usr/local/var/postgres/9.6.2 start",
    "db:local:shell": "psql premiser",
    "create-user:prod": "NODE_ENV=production node bin/create-user.js",
    "create-user:local": "NODE_ENV=development node bin/create-user.js",
    "serve:local": "DEBUG=premiser-api:* NODE_ENV=development DO_ASSERT=true nodemon --watch src --watch ../howdju-models/lib bin/dev-server.js",
    "debug": "DEBUG=premiser-api:* NODE_ENV=development node debug bin/dev-server.js",
    "inspect": "DEBUG=premiser-api:* NODE_ENV=development inspect bin/dev-server.js",
    "local": "yarn run serve:local",
    "localhost": "API_HOST=localhost yarn run local",
    "build": "bin/build.sh",
    "update-lambda-function-code": "npm run build && node bin/update-lambda-function-code.js",
    "publish-lambda-version": "node bin/publish-lambda-version.js",
    "update-lambda-alias": "node bin/update-lambda-alias.js",
    "update-normal-text:local": "NODE_ENV=development node bin/update-normal-text.js",
    "update-normal-text:prod": "NODE_ENV=production node bin/update-normal-text.js",
    "test": "cross-env NODE_ENV=test jest --config .jestrc",
    "test-debug": "node --debug-brk ./node_modules/.bin/jest --runInBand --config .jestrc",
    "test-inspect": "yarn run test:debug &; node-inspector",
    "db:prod:tunnel": "ssh -N -L 5433:premiser.cc4gfeuy5z8h.us-east-1.rds.amazonaws.com:5432 premiser.bastion",
    "db:prod:tunnel:shell": "psql -h 127.0.0.1 -p 5433 -d premiser -U premiser_rds"
  },
  "devDependencies": {
    "argparse": "https://github.com/nodeca/argparse.git",
    "aws-sdk": "^2.34.0",
    "body-parser": "^1.17.1",
    "cross-env": "^4.0.0",
    "debug": "^2.6.3",
    "del": "^2.2.2",
    "express": "^4.15.2",
    "gulp": "^3.9.1",
    "gulp-install": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^4.0.0",
    "inspect-process": "^0.4.2",
    "jest": "^19.0.2",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "read": "^1.0.7",
    "run-sequence": "^1.2.2"
  },
  "dependencies": {
    "argon2": "^0.15.0",
    "bluebird": "^3.5.0",
    "cryptohat": "^1.0.1",
    "deasync": "^0.1.10",
    "moment": "^2.18.1",
    "node-env-file": "^0.1.8",
    "pg": "^6.1.5",
    "urlsafe-base64": "^1.0.0",
    "winston": "^2.3.1"
  }
}
