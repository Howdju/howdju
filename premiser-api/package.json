{
  "name": "premiser-api",
  "version": "0.1.0",
  "description": "API for howdju.com.  Proprietary and confidential.  Copyright Carl Gieringer",
  "main": "./src/index.js",
  "repository": "git@bitbucket.org:howdju/howdju.git",
  "author": "Carl Gieringer <carl@howdju.com>",
  "license": "UNLICENSED",
  "scripts": {
    "build": "../bin/build-and-run-script.sh bin/esbuild.js",
    "build-local-server": "../bin/build-and-run-script.sh bin/esbuild-local-server.js",
    "change-password:local": "env-cmd -f ../config/local.env ../bin/build-and-run-script.sh bin/change-user-password.js",
    "change-password:pre-prod": "env-cmd -f ../config/pre-prod-local-tunnel.env ../bin/build-and-run-script.sh bin/change-user-password.js",
    "change-password:prod": "env-cmd -f ../config/prod-local-tunnel.env ../bin/build-and-run-script.sh bin/change-user-password.js",
    "clean": "rm -rf dist",
    "create-user:prod": "env-cmd -f ../config/prod-local-tunnel.env ../bin/build-and-run-script.sh bin/create-user.js",
    "create-user:pre-prod": "env-cmd -f ../config/pre-prod-local-tunnel.env ../bin/build-and-run-script.sh bin/create-user.js",
    "create-user:local": "env-cmd -f ../config/local.env ../bin/build-and-run-script.sh bin/create-user.js",
    "db:local:create": "docker run -p 5432:5432 --name premiser_postgres postgres:9.6",
    "db:local:restart": "docker restart premiser_postgres",
    "db:local:shell": "psql -h localhost -U postgres premiser",
    "db:local": "yarn run db:local:restart && yarn run db:local:shell",
    "db:tunnel": "ssh -N -L 5433:premiser.cc4gfeuy5z8h.us-east-1.rds.amazonaws.com:5432 bastion.howdju.com",
    "db:tunnel:shell:prod": "psql -h 127.0.0.1 -p 5433 -d premiser -U premiser_rds",
    "db:tunnel:shell:pre-prod": "psql -h 127.0.0.1 -p 5433 -d howdju_pre_prod -U premiser_rds",
    "db:prod": "bin/db-tunnel.sh prod",
    "db:pre-prod": "bin/db-tunnel.sh pre-prod",
    "dedupe-writs:prod": "env-cmd -f ../config/prod-local-tunnel.env ../bin/build-and-run-script.sh bin/dedupe-writs.js",
    "deploy:api": "bin/deploy.sh",
    "inspect-server": "yarn run build-local-server && node --inspect-brk dist/esbuild-local-server.js",
    "lint": "eslint bin src local-server.js",
    "publish-lambda-function-version": "../bin/build-and-run-script.sh bin/publish-lambda-function-version.js",
    "start-server": "yarn run build-local-server && node dist/esbuild-local-server.js",
    "start:api:local": "env-cmd -f ../config/local.env yarn run start-server",
    "start:api:local:inspect": "env-cmd -f ../config/local.env yarn run inspect-server",
    "start:api:localhost": "env-cmd -f ../config/localhost.env yarn run start-server",
    "start:api:localhost:inspect": "env-cmd -f ../config/localhost.env yarn run inspect-server",
    "test": "jest --config=../jest.config.js --testPathPattern=/premiser-api/src/ --testPathIgnorePatterns=/premiser-api/src/config/config\\.test\\.js",
    "test-inspect": "node inspect ../node_modules/.bin/jest --runInBand --watch --config=../jest.config.js --testPathPattern=/premiser-api/src/ --testPathIgnorePatterns=/premiser-api/src/config/config\\.test\\.js",
    "update-lambda-function-code": "../bin/build-and-run-script.sh bin/update-lambda-function-code.js",
    "update-lambda-function-alias": "../bin/build-and-run-script.sh bin/update-lambda-function-alias.js",
    "update-normal-text-local": "env-cmd -f ../config/local.env ../bin/build-and-run-script.sh bin/update-normal-text.js",
    "update-normal-text-pre-prod": "env-cmd -f ../config/pre-prod-local-tunnel.env ../bin/build-and-run-script.sh bin/update-normal-text.js",
    "update-normal-text-prod": "env-cmd -f ../config/prod-local-tunnel.env ../bin/build-and-run-script.sh bin/update-normal-text.js"
  },
  "devDependencies": {
    "argparse": "^2.0.1",
    "artillery": "^1.6.0-21",
    "aws-sdk": "^2.918.0",
    "body-parser": "^1.17.1",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.11.19",
    "eslint": "^7.25.0",
    "eslint-config-howdju": "^1.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "express": "^4.15.2",
    "flow-bin": "^0.150.1",
    "howdju-ops": "^1.0.0",
    "howdju-test-common": "^1.0.0",
    "jest": "^26.6.3",
    "morgan": "^1.8.1",
    "node-zip": "^1.1.1",
    "npm-version": "^1.1.0",
    "private-ip": "^2.3.4",
    "read": "^1.0.7",
    "run-sequence": "^1.2.2"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "dns-sync": "^0.2.1",
    "howdju-common": "^1.0.0",
    "howdju-service-common": "^1.0.0",
    "moment": "^2.18.1",
    "pg": "^8.5.1",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2"
  }
}
