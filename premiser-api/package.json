{
  "name": "premiser-api",
  "version": "0.1.0",
  "description": "API for howdju.com.  Proprietary and confidential.  Copyright Carl Gieringer",
  "main": "index.js",
  "repository": "git@bitbucket.org:howdju/howdju.git",
  "author": "Carl Gieringer <carl@howdju.com>",
  "license": "UNLICENSED",
  "scripts": {
    "build": "gulp build",
    "build-and-update-lambda-function-code": "yarn run build && node bin/update-lambda-function-code.js",
    "change-password:local": "source ../config/local.env && node bin/change-user-password.js",
    "change-password:pre-prod": "source ../config/pre-prod-local-tunnel.env && node bin/change-user-password.js",
    "change-password:prod": "source ../config/prod-local-tunnel.env && node bin/change-user-password.js",
    "check-preconditions": "bin/check-preconditions.sh",
    "clean": "rm -rf dist",
    "create-user:prod": "source ../config/prod-local-tunnel.env && node bin/create-user.js",
    "create-user:pre-prod": "source ../config/pre-prod-local-tunnel.env && node bin/create-user.js",
    "create-user:local": "source ../config/local.env && node bin/create-user.js",
    "db:local:new": "docker run -p 5432:5432 --name premiser_postgres postgres:9.6",
    "db:local:restart": "docker restart premiser_postgres",
    "db:local:shell": "psql -h localhost -U postgres premiser",
    "db:local": "yarn run db:local:restart && yarn run db:local:shell",
    "db:tunnel": "ssh -N -L 5433:premiser.cc4gfeuy5z8h.us-east-1.rds.amazonaws.com:5432 bastion.howdju.com",
    "db:tunnel:shell:prod": "psql -h 127.0.0.1 -p 5433 -d premiser -U premiser_rds",
    "db:tunnel:shell:pre-prod": "psql -h 127.0.0.1 -p 5433 -d howdju_pre_prod -U premiser_rds",
    "db:prod": "bin/db-tunnel.sh prod",
    "db:pre-prod": "bin/db-tunnel.sh pre-prod",
    "dedupe-writs:prod": "source ../config/prod-local-tunnel.env && node bin/dedupe-writs.js",
    "deploy": "yarn run build && bin/deploy.sh",
    "lint": "eslint bin src gulpfile.js server.js",
    "publish-lambda-function-version": "node bin/publish-lambda-function-version.js",
    "start": "yarn run lint && bin/serve.sh",
    "start:local": "source ../config/local.env && yarn start",
    "start:local:debug": "source ../config/local.env && DEBUG=premiser-api:* NODE_ENV=development node debug bin/dev-server.js",
    "start:local:inspect": "source ../config/local.env && DEBUG=premiser-api:* node --inspect-brk bin/dev-server.js",
    "start:local:inspect1": "source ../config/local.env && DEBUG=premiser-api:* inspect bin/dev-server.js",
    "start:local:inspect2": "source ../config/local.env && DEBUG=premiser-api:* node inspect bin/dev-server.js",
    "start:local:inspect3": "source ../config/local.env && DEBUG=premiser-api:* node --inspect --debug-brk bin/dev-server.js",
    "start:localhost": "source ../config/localhost.env && yarn start",
    "start:localhost:inspect": "source ../config/localhost.env && DEBUG=premiser-api:* inspect bin/dev-server.js",
    "test": "cross-env NODE_ENV=test jest --config=../jest.config.js --testPathPattern=/premiser-api/src/ --testPathIgnorePatterns=/premiser-api/src/config/config\\.test\\.js",
    "test:debug": "node debug ./node_modules/.bin/jest --runInBand --config=../jest.config.js --testPathPattern=/premiser-api/src/ --testPathIgnorePatterns=/premiser-api/src/config/config\\.test\\.js",
    "test:inspect": "node --inspect-brk node_modules/.bin/jest --runInBand --watch --config=../jest.config.js --testPathPattern=/premiser-api/src/ --testPathIgnorePatterns=/premiser-api/src/config/config\\.test\\.js",
    "test:inspect1": "yarn run test:debug && node-inspector",
    "test:inspect2": "node --debug-brk --inspect ./node_modules/.bin/jest --runInBand --config=../jest.config.js --testPathPattern=/premiser-api/src/ --testPathIgnorePatterns=/premiser-api/src/config/config\\.test\\.js",
    "update-lambda-function-alias": "node bin/update-lambda-function-alias.js",
    "update-normal-text:local": "source ../config/local.env && node bin/update-normal-text.js",
    "update-normal-text:pre-prod": "source ../config/pre-prod-local-tunnel.env && node bin/update-normal-text.js",
    "update-normal-text:prod": "source ../config/prod-local-tunnel.env && node bin/update-normal-text.js"
  },
  "devDependencies": {
    "argparse": "^2.0.1",
    "artillery": "^1.6.0-21",
    "aws-sdk": "^2.890.0",
    "body-parser": "^1.17.1",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "del": "^2.2.2",
    "express": "^4.15.2",
    "gulp": "4.0",
    "gulp-install": "^1.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^5.0.2",
    "gulp4-run-sequence": "^1.0.1",
    "inspect-process": "^0.4.2",
    "install-local": "^0.6.0",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "read": "^1.0.7",
    "run-sequence": "^1.2.2"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "deasync": "^0.1.10",
    "moment": "^2.18.1",
    "pg": "^8.5.1",
    "uuid": "^8.3.2"
  }
}
