{
  "name": "howdju-mobile-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-release": "react-native run-android --variant release",
    "check": "typecheck && lint && test",
    "ios": "react-native run-ios",
    "ios-release": "react-native run-ios --configuration Release",
    "ios-release-device": "react-native run-ios --configuration Release --device ${0}",
    "bundle": "react-native webpack-bundle",
    "start": "react-native webpack-start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "install-pods": "bin/install-pods.sh",
    "typecheck": "tsc",
    "debugger": "react-devtools"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "add": "^2.0.6",
    "react": "^17.0",
    "react-native": "^0.66",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-share-menu": "file:../../react-native-share-menu",
    "react-native-webview": "^11.15.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@callstack/repack": "^2.5.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^4.4.4",
    "webpack": "^5.73.0"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
