  "scripts": {
    "analyze-size": "  cross-env NODE_ENV=production DEBUG='*,-babel'     webpack --config config/webpack.config.ts --profile --json > dist/stats.json && webpack-bundle-size-analyzer dist/stats.json && open dist/stats.html",
    "analyze-size-2": "cross-env NODE_ENV=production BUNDLE_ANALYZER=yes  webpack --config config/webpack.config.ts",
    "analyze-size-3": "cross-env NODE_ENV=production CHECK_DUPLICATES=yes webpack --config config/webpack.config.ts",
    "analyze-js-size": "zip - dist/premiser-ui.js 2>/dev/null | dd 2>&1 >/dev/null",
    "build:bookmarklet": "rm -rf dist/bookmarklet && mkdir -p dist/bookmarklet && npx babel --no-babelrc bookmarklet/submit.js --out-file dist/bookmarklet/submit.js && npx uglifyjs --compress negate_iife=false --mangle toplevel --output dist/bookmarklet/submit.min.js -- dist/bookmarklet/submit.js",
    "build:bookmarklet:local": "bin/build-bookmarklet-local.sh",
    "build-local": "     cross-env NODE_ENV=development SENTRY_ENV=local    DEBUG='*,-babel,-babel:*'                                               webpack --config config/webpack.config.ts",
    "build-pre-prod": "  cross-env NODE_ENV=production  SENTRY_ENV=pre-prod DEBUG='*,-babel,-babel:*' API_ROOT=https://pre-prod-api.howdju.com/api/ webpack --config config/webpack.config.ts",
    "build-prod": "      cross-env NODE_ENV=production  SENTRY_ENV=prod     DEBUG='*,-babel,-babel:*'                                               webpack --config config/webpack.config.ts",
    "build-prod-local": "cross-env NODE_ENV=production  SENTRY_ENV=local    DEBUG='*,-babel,-babel:*' API_ROOT=http://localhost:8081/api/           webpack --config config/webpack.config.ts",
    "calc-dist-size": "bin/calc-dist-zip-size.sh premiser-ui.js && bin/calc-dist-zip-size.sh main.css",
    "check-format": "yarn run prettier --check --ignore-path .gitignore .",
    "deploy:ui:pre-prod": "bin/deploy.sh pre-prod",
    "deploy:ui:prod": "bin/deploy.sh prod",
    "format": "yarn run lint --fix && yarn run prettier --write --ignore-path .gitignore .",
    "invalidate:ui:pre-prod": "cross-env NODE_ENV=production DEBUG='*' ../bin/build-and-run-script.sh bin/create-invalidation.js E11GMSE61AJBIL",
    "invalidate:ui:prod": "    cross-env NODE_ENV=production DEBUG='*' ../bin/build-and-run-script.sh bin/create-invalidation.js E3ALDHFXYJRKKJ",
    "lint": "eslint --ignore-path=.gitignore .",
    "start:ui:local": "yarn run build-local && yarn run serve-local",
    "serve-local": "     cross-env NODE_ENV=development SENTRY_ENV=local HOST_ADDRESS=localhost             DEBUG='premiser-ui:*' webpack serve --config config/webpack.config.ts",
    "serve-prod-local": "cross-env NODE_ENV=production  SENTRY_ENV=local API_ROOT=http://localhost:8081/api DEBUG='premiser-ui:*' webpack serve --config config/webpack.config.ts",
    "upload-pre-prod": "cross-env NODE_ENV=production DEBUG='*' ../bin/build-and-run-script.sh bin/upload-to-s3.js pre-prod-www.howdju.com",
    "upload-prod": "    cross-env NODE_ENV=production DEBUG='*' ../bin/build-and-run-script.sh bin/upload-to-s3.js www.howdju.com",
    "test": "jest",
    "test-update-snapshot": "jest --updateSnapshot --testNamePattern",
    "test-inspect": "node --inspect-brk ../node_modules/.bin/jest --runInBand --watch",
    "test-verbose": "jest --verbose",
    "test-dns": "dig +recurse +trace howdju.com any",
    "typecheck": "tsc"
  },
