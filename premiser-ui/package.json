  "license": "AGPL-3.0-or-later",
  "scripts": {
    "analyze-duplicates": "cross-env NODE_ENV=production DEBUG='*,-babel' CHECK_DUPLICATES=yes webpack --config config/webpack.config.ts",
    "analyze-js-size": "zip - dist/premiser-ui.js 2>/dev/null | dd 2>&1 >/dev/null",
    "analyze-size-statoscope": "cross-env NODE_ENV=production DEBUG='*,-babel' STATOSCOPE=yes       webpack --config config/webpack.config.ts --profile --json=dist/stats.json",
    "analyze-size-text": "cross-env NODE_ENV=production DEBUG='*,-babel'                      webpack --config config/webpack.config.ts --profile --json=dist/stats.json && webpack-bundle-size-analyzer dist/stats.json && open dist/stats.html",
    "analyze-size-tree-map": "cross-env NODE_ENV=production DEBUG='*,-babel' BUNDLE_ANALYZER=yes  webpack --config config/webpack.config.ts",
    "check-build": "yarn run build-local",
    "build-local": "        cross-env NODE_ENV=development SENTRY_ENV=local    DEBUG='*,-babel,-babel:*'                                               webpack --config config/webpack.config.ts",
    "build-local-network": "cross-env NODE_ENV=development SENTRY_ENV=local    DEBUG='*,-babel,-babel:*' DEV_SERVER_LOCAL_NETWORK=yes                  webpack --config config/webpack.config.ts",
    "build-pre-prod": "     cross-env NODE_ENV=production  SENTRY_ENV=pre-prod DEBUG='*,-babel,-babel:*' API_ROOT=https://pre-prod-api.howdju.com/api/ webpack --config config/webpack.config.ts",
    "build-prod": "         cross-env NODE_ENV=production  SENTRY_ENV=prod     DEBUG='*,-babel,-babel:*'                                               webpack --config config/webpack.config.ts",
    "build-prod-local": "   cross-env NODE_ENV=production  SENTRY_ENV=local    DEBUG='*,-babel,-babel:*' USE_PROD_LOCAL_API_ROOT=true                  webpack --config config/webpack.config.ts",
    "calc-dist-size": "bin/calc-dist-zip-size.sh premiser-ui.js && bin/calc-dist-zip-size.sh main.css",
    "check-format": "yarn run prettier --check --ignore-path .gitignore .",
    "deploy:ui:pre-prod": "bin/deploy.sh pre-prod",
    "deploy:ui:prod": "bin/deploy.sh prod",
    "fix-format": "yarn run lint --fix && yarn run prettier --write --ignore-path .gitignore .",
    "invalidate:ui:pre-prod": "cross-env NODE_ENV=production DEBUG='*' ../bin/build-and-run-script.sh bin/create-invalidation.js E11GMSE61AJBIL",
    "invalidate:ui:prod": "    cross-env NODE_ENV=production DEBUG='*' ../bin/build-and-run-script.sh bin/create-invalidation.js E3ALDHFXYJRKKJ",
    "lint": "eslint --ignore-path=.gitignore .",
    "start:ui:local": "        yarn run build-local         && yarn run serve-local",
    "start:ui:local:prod": "   yarn run build-prod-local    && yarn run serve-prod-local",
    "start:ui:local-network": "yarn run build-local-network && yarn run serve-local",
    "serve-local": "     cross-env NODE_ENV=development SENTRY_ENV=local DEBUG='premiser-ui:*'                              webpack serve --config config/webpack.config.ts",
    "serve-prod-local": "cross-env NODE_ENV=production  SENTRY_ENV=local DEBUG='premiser-ui:*' USE_PROD_LOCAL_API_ROOT=true webpack serve --config config/webpack.config.ts",
    "upload-pre-prod": "cross-env NODE_ENV=production DEBUG='*' ../bin/build-and-run-script.sh bin/upload-to-s3.js pre-prod-www.howdju.com",
    "upload-prod": "    cross-env NODE_ENV=production DEBUG='*' ../bin/build-and-run-script.sh bin/upload-to-s3.js www.howdju.com",
    "test": "TZ=UTC jest",
    "test-update-snapshot": "TZ=UTC jest --updateSnapshot --testNamePattern",
    "test-inspect": "TZ=UTC node --inspect-brk ../node_modules/.bin/jest --runInBand --watch",
    "test-verbose": "TZ=UTC jest --verbose",
    "test-dns": "dig +recurse +trace howdju.com any",
    "typecheck": "tsc"
  },
