{
  "name": "premiser-ui",
  "version": "0.1.0",
  "description": "UI for premiser.co.  Proprietary and confidential.  Copyright Carl Gieringer",
  "main": "index.js",
  "repository": "git@bitbucket.org:premiser/premiser-ui.git",
  "author": "Carl Gieringer <carl@premiser.co>",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rm -rf dist",
    "build:dev": "yarn run clean && cross-env NODE_ENV=development DEBUG=* webpack --config config/webpack.config.js",
    "build:prod": "yarn run clean && cross-env NODE_ENV=production DEBUG=* webpack --config config/webpack.config.js",
    "serve:dev": "cross-env NODE_ENV=development DEBUG=premiser-ui:* node bin/dev-server.js",
    "serve:prod": "cross-env NODE_ENV=production DEBUG=premiser-ui:* node bin/dev-server.js",
    "dev": "yarn run build:dev && yarn run serve:dev",
    "invalidate": "cross-env NODE_ENV=production DEBUG=* node bin/create-invalidation.js",
    "upload": "cross-env NODE_ENV=production DEBUG=* node bin/upload-to-s3.js",
    "deploy": "yarn run build:prod && cp src/error.html dist/ && yarn run upload && yarn run invalidate",
    "test": "cross-env NODE_ENV=test jest",
    "test-dns": "dig +recurse +trace www.premiser.co any"
  },
  "devDependencies": {
    "aws-sdk": "^2.35.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-dynamic-import-node": "^1.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "debug": "^2.6.1",
    "express": "^4.14.1",
    "fs-extra": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "next",
    "style-loader": "^0.14.1",
    "uuid": "^3.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.2",
    "react": "^15.4.2",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-persist": "^4.6.0",
    "redux-saga": "^0.14.3"
  },
  "jest": {
    "testRegex": "(/src/.*\\.test\\.js)$"
  }
}
