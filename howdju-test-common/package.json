{
  "name": "howdju-test-common",
  "version": "1.0.0",
  "description": "Common test utilities",
  "main": "lib/index.ts",
  "author": "carl.gieringer@gmail.com",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "check-format": "yarn run prettier --check --ignore-path .gitignore .",
    "fix-format": "yarn run lint --fix && yarn run prettier --write --ignore-path .gitignore .",
    "lint": "eslint --ignore-path=.gitignore .",
    "test": "jest",
    "test-inspect": "node --inspect-brk ../node_modules/.bin/jest --runInBand --watch"
  },
  "dependencies": {
    "sinon": "^10.0.0"
  },
  "devDependencies": {
    "@types/sinon": "^10.0.13",
    "eslint": "^8.27.0",
    "eslint-config-howdju": "workspace:eslint-config-howdju",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.4",
    "lodash": "^4.17.21",
    "prettier": "2.7.1",
    "ts-node": "^10.9.1"
  }
}
